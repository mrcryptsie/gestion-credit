{
    "version": 2,
    "builds": [
        {
            "src": "loan-management-system-django-master/loan_management_system/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        },
        {
            "src": "loan-management-system-django-master/staticfiles/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "loan-management-system-django-master/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "loan-management-system-django-master/loan_management_system/wsgi.py"
        }
    ]
}
